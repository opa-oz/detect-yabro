{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2016"
    ],
    "types": [
      "node",
      "jest"
    ],
    "module": "commonjs",
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "outDir": "./lib"
  }
}
